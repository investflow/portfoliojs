{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "curly": 1,
    "no-cond-assign": 2,
    "no-unused-vars": 1,
    "no-var": 1,
    "prefer-arrow-callback": 1,
    "quotes": [1, "double"],
    "yoda": 2
  }
}