    "highcharts": "5.0.14",
    "highlight.js": "9.12.0",
    "jquery": "v=1.12.4",
    "jquery-flot": "0.8.3",
    "loglevel": "1.4.1",
    "lz-string": "1.4.4",
    "mustache": "2.3.0",
    "parsleyjs": "2.7.2",
    "store": "2.0.12"
  },
  "devDependencies": {
    "html-loader": "0.5.0",
    "ts-loader": "2.3.2",
    "typescript": "2.4.2",
    "webpack": "3.4.1",
